{
  "name": "fieldstack",
  "version": "0.1.5",
  "description": "A declarative library for building beautiful, robust & flexible form-based interfaces",
  "main": "lib/index.js",
  "module": "es/index.js",
  "repository": "https://github.com/buchanora/fieldstack.git",
  "author": "Buchanora",
  "license": "MIT",
  "scripts": {
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build": "node ./tools/build.js && npm run bundleScss",
    "watch": "babel ./modules -d . --ignore __tests__ --watch",
    "prepublishOnly": "npm run build",
    "bundleScss": "bundle-scss -m \"./src/styles.manifest.scss\" -d \"./lib/styles.scss\"",
    "buildScss": "scss-bundle -e ./src/styles.scss -d ./lib/bundle.scss",
    "clean": "git clean -fdX .",
    "lint": "eslint src",
    "test": "jest",
    "postinstall": "node postinstall.js"
  },
  "peerDependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "redux": "^3.7.2"
  },
  "dependencies": {
    "draft-js": "^0.10.1",
    "draft-js-export-html": "^1.2.0",
    "draft-js-export-markdown": "^1.2.0",
    "draft-js-plugins-editor": "^2.0.3",
    "prop-types": "^15.6.0"
  },
  "devDependencies": {
    "@storybook/react": "^3.4.1",
    "autoprefixer": "^8.2.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-imports": "^1.2.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.12",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "bundle-scss": "^1.3.3",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "eslint": "^4.16.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.6.0",
    "gzip-size": "^4.1.0",
    "jest": "^22.1.4",
    "node-sass": "^4.8.3",
    "postcss": "^6.0.21",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.3",
    "pretty-bytes": "^4.0.2",
    "raf": "^3.4.0",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "redux": "^3.7.2",
    "rollup": "^0.50.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-collect-sass": "^1.0.9",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-image": "^1.0.2",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-sass": "^0.6.0",
    "rollup-plugin-strip-code": "^0.2.5",
    "rollup-plugin-uglify": "^3.0.0",
    "sass-loader": "^7.0.1",
    "scss-bundle": "^2.1.3",
    "semver": "^5.5.0",
    "style-loader": "^0.20.3",
    "sugarss": "^1.0.1"
  },
  "browserify": {
    "transform": [
      "loose-envify"
    ]
  },
  "jest": {
    "setupFiles": [
      "raf/polyfill"
    ]
  },
  "keywords": [
    "react",
    "forms",
    "library",
    "redux",
    "fields",
    "editor"
  ]
